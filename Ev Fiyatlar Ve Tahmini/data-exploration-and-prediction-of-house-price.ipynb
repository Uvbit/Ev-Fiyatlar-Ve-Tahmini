   "execution_count": null,
   "outputs": []
  },
  {
   "cell_type": "markdown",
   "source": [
    "<font face=\"calibri\" size=\"4\">Step 4: forecast and check for model accuracy"
   ],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "source": [
    "library(forecast)\n",
    "#use predict() to make prediction on a new set\n",
    "pred1 <- predict(linreg,model_lin_valid,type = \"response\")\n",
    "residuals <- model_lin_valid$lSalePrice - pred1\n",
    "linreg_pred <- data.frame(\"Predicted\" = pred1, \"Actual\" = model_lin_valid$lSalePrice, \"Residual\" = residuals)\n",
    "accuracy(pred1, model_lin_valid$lSalePrice)"
   ],
